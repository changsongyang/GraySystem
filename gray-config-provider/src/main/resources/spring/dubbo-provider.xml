<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">



    <dubbo:service
            interface="com.facishare.wechat.gray.api.service.GrayApplicationEaInfoService"
            ref="grayApplicationEaInfoServiceImpl"
            provider="wechatGrayProvider"
            version="1.0"/>

    <dubbo:service
            interface="com.facishare.wechat.gray.api.service.GrayApplicationGroupConfigService"
            ref="grayApplicationGroupConfigServiceImpl"
            provider="wechatGrayProvider"
            version="1.0"/>

    <dubbo:service
            interface="com.facishare.wechat.gray.api.service.GrayApplicationInfoService"
            ref="grayApplicationInfoServiceImpl"
            provider="wechatGrayProvider"
            version="1.0"/>

    <dubbo:service
            interface="com.facishare.wechat.gray.api.service.GrayApplicationIpConfigService"
            ref="grayApplicationIpConfigServiceImpl"
            provider="wechatGrayProvider"
            version="1.0"/>

    <dubbo:service
            interface="com.facishare.wechat.gray.api.service.GrayApplicationHandlerService"
            ref="grayApplicationHandlerServiceImpl"
            provider="wechatGrayProvider"
            version="1.0"/>
</beans>
